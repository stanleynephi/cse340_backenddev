<h1>
    Welcome to CSE Motors
</h1>
<%- messages() %>

    <section class="message_button">
        <div class="overlay">
            <div class="content">
                <h2><span>DMC</span> Delorean</h2>
                <ul>
                    <li>3 Cup holders</li>
                    <li>Superman doors</li>
                    <li>Fuzzy dice</li>
                </ul>
                <button>Own Today</button>
            </div>
        </div>
    </section>


    <section class="upgrades_reviews">
        <section class="upgrades">
            <h1>Delorean Upgrades</h1>
            <div class="upgradeimages">
                <ul>
                    <li><img src="/images/upgrades/flux-cap.png" alt="Flux Capacitor">
                        <a href="#">Flux Capacitor</a>
                    </li>
                    <li><img src="/images/upgrades/flame.jpg" alt="Flame Decal">
                        <a href="#">Flame Decal</a>
                    </li>
                    <li><img src="/images/upgrades/bumper_sticker.jpg" alt="Bumper Stickers">
                        <a href="#">Bumper Stickers</a>
                    </li>
                    <li><img src="/images/upgrades/hub-cap.jpg" alt="Hub Caps">
                        <a href="#">Hub Caps</a>
                    </li>
                </ul>
            </div>
        </section>

        <section class="reviews">
            <h1>Delorean Reviews</h1>
            <div class="review_comments">
                <ul>
                    <li>"So fast it's almost like travelling in time" (4/5)</li>
                    <li>"Coolest ride on the road" (4/5)</li>
                    <li>"I'm feeling McFly" (5/5)</li>
                    <li>"80's livin and I love it" (5/5)</li>
                </ul>
            </div>
        </section>
    </section>